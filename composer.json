{
    "name": "skaut/skautske_hospodareni",
    "type": "project",
    "require": {
        "php": ">= 7.4",
        "beberlei/assert": "^2.7",
        "beberlei/doctrineextensions": "^1.0",
        "cakephp/chronos": "^2.3",
        "composer/ca-bundle": "^1.1",
        "consistence-community/consistence": "^2.1",
        "consistence-community/consistence-doctrine": "^2.1",
        "contributte/console": "^0.9.0",
        "fmasa/doctrine-nullable-embeddables": "^1.0",
        "fmasa/messenger": "^1.1",
        "fmasa/sentry-breadcrumbs-monolog-handler": "^2.0",
        "google/apiclient": "^2.8",
        "heureka/bank-account-validator": "1.1.*",
        "http-interop/http-factory-guzzle": "^1.0",
        "kdyby/forms-replicator": "^2.0",
        "latte/latte": " ~2.4",
        "league/flysystem": "^2.4",
        "maennchen/zipstream-php": "^2.0",
        "mhujer/fio-api-php": "^4.0",
        "moneyphp/money": "^3.0",
        "monolog/monolog": "^2.0",
        "mpdf/mpdf": "^8.0",
        "nasext/dependent-select-box": "^5.0",
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.1",
        "nette/caching": " ~3.0",
        "nette/di": " ^3.0",
        "nette/forms": " ^3.1",
        "nette/http": "^3.1",
        "nette/mail": " ^3.1",
        "nette/robot-loader": "^3.0",
        "nette/security": "^3.1",
        "nette/utils": "^3.2",
        "nettrine/cache": "^0.3.0",
        "nettrine/dbal": "^0.7",
        "nettrine/migrations": "^v0.7",
        "nextras/mail-panel": "^v2.5",
        "php-http/guzzle6-adapter": "^2.0",
        "phpoffice/phpspreadsheet": "^1.0",
        "ramsey/uuid": "^4.0",
        "sentry/sentry": "^3.1",
        "skautis/nette": "^2.1",
        "skautis/skautis": "2.*@dev",
        "thecodingmachine/safe": "^1.0.0",
        "tracy/tracy": "~2.4",
        "ublaboo/datagrid": "^v6.8",
        "warhuhn/chronos-doctrine": "^3.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/mockery-module": "^0.4.0",
        "codeception/module-asserts": "^2.0",
        "codeception/module-db": "^2.0",
        "codeception/module-webdriver": "^2.0",
        "contributte/codeception": "^1.3.1",
        "doctrine/coding-standard": "^9.0",
        "league/flysystem-memory": "^2.0",
        "phing/phing": "^2.17.2",
        "php-vcr/php-vcr": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-nette": "^1.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload-dev": {
        "classmap": ["code-quality/", "tests/_hacks"]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "post-update-cmd": "Google_Task_Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Gmail",
            "Oauth2"
        ]
    }
}
