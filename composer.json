        "warhuhn/chronos-doctrine": "v4.1.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/mockery-module": "^0.4.0",
        "codeception/module-asserts": "^2.0",
        "codeception/module-db": "^2.0",
        "codeception/module-webdriver": "^2.0",
        "contributte/codeception": "^1.3.1",
        "doctrine/coding-standard": "^12.0",
        "league/flysystem-memory": "^3.10",
        "phing/phing": "^2.17.2",
        "php-vcr/php-vcr": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-nette": "^1.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6",
        "ext-pdo": "*"
    },
    "autoload-dev": {
        "classmap": ["code-quality/", "tests/_hacks"]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "post-update-cmd": "Google_Task_Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Gmail",
            "Oauth2"
        ]
    }
